{
  "name": "mern-minimal",
  "version": "1.0.0",
  "description": "A simple MongoDB Express.js React.js Node.js (MERN) universal starter kit with code splitting + hot reloading",
  "main": "index.js",
  "sideEffects": [
    "*.css"
  ],
  "browserslist": [
    "last 3 versions"
  ],
  "scripts": {
    "clean:dist": "shx rm -rf ./dist",
    "start:dev": "npm run clean:dist && cross-env NODE_ENV=development node ./index.js",
    "start:prod": "cross-env NODE_ENV=production APP_ENV=production node ./index.js",
    "start:stage": "cross-env NODE_ENV=production APP_ENV=staging node ./index.js",
    "build:client": "cross-env NODE_ENV=production APP_ENV=production webpack --config webpack.config.client.prod.js",
    "build:server": "cross-env NODE_ENV=production APP_ENV=production webpack --config webpack.config.server.prod.js",
    "build:client:stage": "cross-env NODE_ENV=production APP_ENV=staging webpack --config webpack.config.client.prod.js",
    "build:server:stage": "cross-env NODE_ENV=production APP_ENV=staging webpack --config webpack.config.server.prod.js",
    "build:start:prod": "npm run clean:dist && npm run build:client && npm run build:server && npm run start:prod",
    "build:start:stage": "npm run clean:dist && npm run build:client:stage && npm run build:server:stage && npm run start:stage",
    "build:prod": "npm run clean:dist && npm run build:client && npm run build:server",
    "build:stage": "npm run clean:dist && npm run build:client:stage && npm run build:server:stage",
    "client:test": "jest --coverage || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitrecipe/mern-minimal.git"
  },
  "bugs": {
    "url": "https://github.com/bitrecipe/mern-minimal/issues"
  },
  "private": true,
  "author": "Subeesh M.M <subeeshmm.open@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
